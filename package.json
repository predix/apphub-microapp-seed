    "react-popper": "^0.10.4",
    "react-svg-loader": "^2.1.0",
    "redis-mock": "^0.21.0",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "selenium-server-standalone-jar": "^3.7.1",
    "sinon": "^1.17.6",
