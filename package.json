    "connect-redis": "^3.3.3",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.3",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.2.4",
    "es6-shim": "^0.35.3",
    "express": "^4.14.0",
    "express-api-routes-list": "^1.1.2",
    "express-controller-routing": "^1.0.0",
    "express-request-proxy": "^2.0.0",
    "express-session": "^1.15.6",
    "fs-extra": "^1.0.0",
    "home-or-tmp": "^3.0.0",
    "i18next": "^3.4.3",
    "i18next-express-middleware": "^1.0.2",
    "ioredis": "^3.2.2",
    "lowdb": "^1.0.0",
    "pascal-case": "^2.0.1",
    "passport": "^0.4.0",
    "passport-oauth2-middleware": "^1.0.3",
    "passport-predix-oauth": "^0.1.15",
    "predix-fast-token": "^1.2.1",
    "predix-ui": "^0.0.22",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-router-dom": "^4.2.2",
    "request": "^2.79.0",
    "serve-static": "^1.10.2",
    "socket.io": "^1.5.1",
    "uuid": "^3.2.1",
    "whatwg-fetch": "^2.0.4"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "include": ["src/server/**/*.js"],
    "exclude": ["test/server/**/*-spec.js", "test/server/**/*.spec.js"],
    "reporter": ["lcov", "text-summary"],
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>__tests__/setup.js",
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage/client",
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "testMatch": [
      "<rootDir>/__tests__/**/*.test.js?(x)",
      "<rootDir>/src/components/**/*.test.js?(x)",
      "<rootDir>/src/containers/**/*.test.js?(x)",
      "<rootDir>/src/pages/**/*.test.js?(x)"
    ],
    "moduleFileExtensions": ["js", "jsx"],
    "modulePathIgnorePatterns": ["__tests__", "public", "tools", "stories", "docs"],
    "moduleDirectories": ["node_modules"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
