  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "include": [
      "src/server/**/*.js"
    ],
    "exclude": [
      "test/server/**/*-spec.js",
      "test/server/**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  },
  "jest": {
    "testURL": "http://localhost/",
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup.js"
    ],
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/__tests__/**/*.test.js?(x)",
      "<rootDir>/src/components/**/*.test.js?(x)",
      "<rootDir>/src/containers/**/*.test.js?(x)",
      "<rootDir>/src/pages/**/*.test.js?(x)",
      "<rootDir>/test/server/**/*-spec.js"
    ],
    "modulePathIgnorePatterns": [
      "__tests__",
      "public",
      "tools",
      "stories",
      "docs"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
