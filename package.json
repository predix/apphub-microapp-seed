{
  "name": "apphub-microapp-seed",
  "version": "1.0.0",
  "repository": "https://github.build.ge.com/predix-apphub/apphub-microapp-seed",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "heroku-prebuild": "echo This runs before Heroku installs your dependencies.",
    "heroku-postbuild": "echo This runs afterwards.",
    "bower": "bower install --force --allow-root",
    "build": "cross-env NODE_ENV=production webpack --mode=production --progress",
    "postbuild": "rimraf dist/stats.json",
    "build:old": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cross-env NODE_ENV=production BABEL_ENV=client webpack --progress",
    "build:backend": "cross-env NODE_ENV=production BUILD_TARGET=backend BABEL_ENV=server webpack --progress",
    "build:backend:lib": "cross-env NODE_ENV=production BUILD_AS_LIB=true BUILD_TARGET=backend webpack -p",
    "copy": "cpy 'Dockerfile' 'package.json' 'manifest.yml' 'config.js' '*.md' ./dist --parents",
    "clean": "rimraf dist $npm_package_name-$npm_package_version.zip",
    "docs": "esdoc",
    "predeploy": "npm run zip",
    "deploy": "cf push -f ./dist/manifest.yml -p $npm_package_name-$npm_package_version.zip",
    "predist": "npm run clean",
    "dist": "npm run bower && npm run build && npm run copy && npm run zip",
    "dev": "cross-env NODE_ENV=development node src/server",
    "docker:build": "docker build -t predixapphub/$npm_package_name .",
    "docker:run": "docker run -p 49161:8080 -d predixapphub/$npm_package_name",
    "docker:save": "docker save -o $npm_package_name.tar predixapphub/$npm_package_name",
    "start": "cross-env NODE_ENV=production node server",
    "pretest": "rimraf coverage && npm run lint:fix",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:e2e": "cross-env NODE_ENV=test nightwatch --config ./test/e2e/nightwatch.conf.js",
    "lint": "prettier -c ./src/**/*.js ./src/**/*.jsx",
    "lint:fix": "prettier --write ./src/**/*.js ./src/**/*.jsx",
    "zip": "cd dist && bestzip ../$npm_package_name-$npm_package_version.zip *",
    "storybook": "start-storybook -p 9090 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o .out"
  },
  "keywords": [
    "predix-microapp"
  ],
