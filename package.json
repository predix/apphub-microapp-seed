{
  "name": "apphub-microapp-seed",
  "version": "1.0.0",
  "repository": "https://github.build.ge.com/predix-apphub/apphub-microapp-seed",
  "private": true,
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "bower": "bower install --force --allow-root",
    "prebuild": "rimraf build dist",
    "build": "cross-env NODE_ENV=production webpack --mode production --progress",
    "build:old": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cross-env NODE_ENV=production BABEL_ENV=client webpack --progress",
    "build:backend": "cross-env NODE_ENV=production BUILD_TARGET=backend BABEL_ENV=server webpack --progress",
    "build:backend:lib": "cross-env NODE_ENV=production BUILD_AS_LIB=true BUILD_TARGET=backend webpack -p",
    "copy": "cpy 'Dockerfile' 'package.json' 'manifest.yml' 'config.js' '*.md' 'public/**/*.*' ./dist --parents",
    "clean": "rimraf coverage dist .nyc_output build",
    "docs": "esdoc",
    "predeploy": "npm test",
    "deploy": "npm run dist && cd ./dist && cf push",
    "predist": "rimraf dist",
    "dist": "npm run bower && npm run build && npm run copy && npm run zip",
    "dev": "cross-env NODE_ENV=development node src/server",
    "docker:build": "docker build -t predix-apphub/$npm_package_name .",
    "docker:run": "docker run -p 49161:8080 -d predix-apphub/$npm_package_name",
    "docker:save": "docker save -o $npm_package_name.tar predix-apphub/$npm_package_name",
    "start": "cross-env NODE_ENV=production node src/server",
    "pretest": "rimraf coverage && npm run lint",
    "test": "npm run test:server && npm run test:client",
    "test:client": "cross-env NODE_ENV=test jest --coverage",
    "test:server": "cross-env NODE_ENV=test nyc mocha test/server/**/*-spec.js",
    "test:e2e": "cross-env NODE_ENV=test nightwatch --config ./test/e2e/nightwatch.conf.js",
    "lint": "eslint ./src ./test --cache",
    "zip": "cd dist && bestzip ../$npm_package_name-$BUILD_NUMBER.zip *",
    "storybook": "start-storybook -p 9090 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o .out"
  },
  "keywords": ["predix-microapp"],
  "devDependencies": {
    "@storybook/addon-knobs": "^3.4.1",
    "@storybook/addon-viewport": "^3.4.1",
    "@storybook/addons": "^3.4.1",
    "@storybook/react": "^3.4.1",
    "autoprefixer": "^7.2.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "bestzip": "^1.1.4",
    "bower": "^1.8.2",
    "caniuse-lite": "^1.0.30000828",
    "cdnizer": "^1.1.6",
    "chai": "^3.5.0",
    "chai-jest-snapshot": "^2.0.0",
    "chai-shallow-deep-equal": "^1.4.4",
    "chromedriver": "^2.35.0",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.3.1",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.28.9",
    "cssnano": "^3.10.0",
    "dotenv-webpack": "^1.5.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "esdoc": "^1.0.3",
